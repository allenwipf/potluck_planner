<% if @potluck %>
<% if @potluck.errors.any? %>

  <div id="error_explanation">
    <div class="alert alert-danger">
      The form contains <%= pluralize(@potluck.errors.count, "error") %>.
    </div>
    <ul>
    <% @potluck.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
<% end %>